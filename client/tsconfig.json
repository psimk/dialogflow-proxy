{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck":true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["DOM", "ES7", "DOM.Iterable", "ScriptHost"],
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "include": [
    "src"
  ]
}
